{
    "printWidth": 100,
    "jsxBracketSameLine": true,
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "useTabs": false,
    "overrides": [
        {
            "files": [".prettierrc", "*.json"],
            "options": {
                "parser": "json"
            }
        },
        {
            "files": "*.scss",
            "options": {
                "parser": "scss",
                "singleQuote": true
            }
        }
    ]
}
